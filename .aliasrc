# Navigation
alias wd="cd /mnt/e/WebDev"

# Config files
alias zshrc="$EDITOR ~/.zshrc"
alias vimrc="$EDITOR ~/.vimrc"
alias aliasrc="$EDITOR ~/.aliasrc"

# Git stuff
gitcommit() {
    git add .
    git commit
}
gitupdate() {
    eval "$(ssh-agent -s)"
    ssh-add ~/.ssh/id_ecdsa_github
    ssh -T git@github.com
} 
gitpush() {
    eval "$(ssh-agent -s)"
    ssh-add ~/.ssh/id_ecdsa_github
    ssh -T git@github.com
    git pull
    git push
}

alias gs="git status"
alias gc=gitcommit
alias gp=gitpush
alias gu=gitupdate 

# Deno
alias deno=~/.deno/bin/deno

# Python stuff
alias pye="source env/bin/activate"

# NPM stuff
alias ns="npm start"
alias nd="npm run dev"
alias nb="npm run build"

# Other
alias h=history

