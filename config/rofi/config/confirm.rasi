@import "font.rasi"
@import "catppuccin-mocha-colors.rasi"

* {
  background-color: @bg-col;
  text-color: @fg-col;
}

window {
  width: 300px;
  padding: 20px;
  border: 0px 0px 2px 0px;
  border-radius: 8px;
  border-color: @border-col;
  location: 0;
  x-offset: 0;
  y-offset: -4%;
}

entry {
  expand: true;
  width: 250px;
}
